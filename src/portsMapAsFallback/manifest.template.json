{
  "manifest_version": 3,
  "name": "Ports map as fallback",
  "version": "1.0",
  "description": "При доступа к машине на определенный недоступный порт, пользователю отображается таблица портов.",
  "icons": {
    "16": "assets/icons/favicon-16x16.png",
    "32": "assets/icons/favicon-32x32.png",
    "48": "assets/icons/favicon-48x48.png",
    "128": "assets/icons/favicon-48x48.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Ports map as fallback"
  },
  "web_accessible_resources": [
    {
      "resources": ["./results.html", "./results.js", "./scanner.js", "./styles.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
